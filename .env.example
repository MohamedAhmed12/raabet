# Environment variables declared in this file are automatically made available to Prisma.
# See the documentation for more detail: https://pris.ly/d/prisma-schema#accessing-environment-variables-from-the-schema

# Prisma supports the native connection string format for PostgreSQL, MySQL, SQLite, SQL Server, MongoDB and CockroachDB.
# See the documentation for all the connection string options: https://pris.ly/d/connection-strings

# Sentry
NEXT_PUBLIC_APP_ENV="development"
NEXT_PUBLIC_SENTRY_DSN=""
NEXT_PUBLIC_ENABLE_SENTRY_IN_DEV="true"

NEXT_PUBLIC_BASE_URL="http://localhost:3000"
NEXT_PUBLIC_BASE_URL_LABEL="localhost:3000"

DATABASE_URL="postgresql://postgres:1234@localhost:5432/mydb?pgbouncer=true"
DIRECT_URL="postgresql://postgres:1234@localhost:5432/mydb"

# next auth 
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your_random_secret"


# mail trap
MAIL_HOST=
MAIL_PORT=
MAIL_USER=
MAIL_PASS=
MAIL_SECURE="false"
NEXT_PUBLIC_SUPPORT_EMAIL="support@rabet-link.com"

# google storage bucket 
GCP_PROJECT_ID=
GCP_CLIENT_EMAIL=
GCP_PRIVATE_KEY=
GCS_BUCKET_NAME=

# google analytics
NEXT_PUBLIC_GA_ID="your_google_analytics_ID"

# stripe 
NEXT_PUBLIC_STRIPE_ENG_PRICING_TABLE_ID=
NEXT_PUBLIC_STRIPE_AR_PRICING_TABLE_ID=
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=
NEXT_PUBLIC_STRIPE_SECRET_KEY=
NEXT_PUBLIC_STRIPE_WEBHOOK_SECRET=
NEXT_PUBLIC_STRIPE_DISCOUNT=
NEXT_PUBLIC_SUBSCRIPTION_VALUE=

# JWT
JWT_SECRET=

# google search console
NEXT_PUBLIC_GOOGLE_SEARCH_CONSOLE_KEY=

# TikTok Pixel (optional - leave empty to disable)
NEXT_PUBLIC_TIKTOK_PIXEL_ID=

# Telegram (admin notifications for new registrations)
# Create a bot via @BotFather, then get your chat ID (e.g. from @userinfobot or getUpdates)
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=