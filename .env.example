# Sentry
NEXT_PUBLIC_APP_ENV="development"
NEXT_PUBLIC_SENTRY_DSN=""
NEXT_PUBLIC_ENABLE_SENTRY_IN_DEV="true"

NEXT_PUBLIC_BASE_URL="http://localhost:3000"
NEXT_PUBLIC_BASE_URL_LABEL="localhost:3000"

DATABASE_URL="postgresql://postgres:1234@localhost:5432/mydb?pgbouncer=true"
DIRECT_URL="postgresql://postgres:1234@localhost:5432/mydb"

# next auth 
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your_random_secret"


# mail trap
MAIL_HOST=
MAIL_PORT=
MAIL_USER=
MAIL_PASS=
MAIL_SECURE="false"
NEXT_PUBLIC_SUPPORT_EMAIL="support@rabet-link.com"

# google storage bucket 
GCP_PROJECT_ID=
GCP_CLIENT_EMAIL=
GCP_PRIVATE_KEY=
GCS_BUCKET_NAME=

# google analytics
NEXT_PUBLIC_GA_ID="your_google_analytics_ID"

# stripe 
NEXT_PUBLIC_STRIPE_ENG_PRICING_TABLE_ID=
NEXT_PUBLIC_STRIPE_AR_PRICING_TABLE_ID=
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=
NEXT_PUBLIC_STRIPE_SECRET_KEY=
NEXT_PUBLIC_STRIPE_WEBHOOK_SECRET=
NEXT_PUBLIC_STRIPE_DISCOUNT=
NEXT_PUBLIC_SUBSCRIPTION_VALUE=
NEXT_PUBLIC_STRIPE_COUPON_ID=

# supabase
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
